{
  "name": "warframe-name-generator",
  "version": "0.0.0-development",
  "description": "Name Generator for Warframe",
  "keywords": [
    "warframe",
    "name generator",
    "warframe name generator"
  ],
  "license": "Apache-2.0",
  "author": "TobiTenno",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/Generator.js"
    },
    "./data": {
      "import": "./lib/data.js"
    }
  },
  "scripts": {
    "fmt": "npx prettier --check .",
    "fmt:fix": "npx prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky && npx install-peerdeps @wfcd/eslint-config@latest -S",
    "test": "c8 mocha"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "c8": "^10.1.2",
    "chai": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "mocha": "^11.1.0",
    "sinon": "^20.0.0"
  },
  "engines": {
    "node": ">=18.19.1",
    "npm": ">=10.2.4"
  }
}
